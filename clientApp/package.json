{
  "name": "getheadlines-client-app",
  "version": "1.0.0",
  "description": "Automates XML/RSS feed parsing into JSON and exposes data via REST API",
  "main": "index.js",
  "scripts": {
    "cleardist": "rimraf ./public/*",
    "lint": "xo *.js",
    "build": "yarn cleardist && NODE_ENV=production webpack -p",
    "start": "node_modules/.bin/webpack-dev-server",
    "start:firebase": "firebase serve --only hosting"
  },
  "author": "Mladen Ruzicic <ruzicic@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "v4.0.0-alpha.6",
    "bootstrap-loader": "2.1.0",
    "css-loader": "0.28.4",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "0.11.2",
    "global": "4.3.2",
    "html-webpack-plugin": "2.29.0",
    "image-webpack-loader": "3.3.1",
    "imports-loader": "^0.7.1",
    "node-sass": "4.5.3",
    "postcss": "^6.0.6",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "2.1.0",
    "resource-hints-webpack-plugin": "^0.0.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "3.1.0",
    "webpack-dev-server": "2.5.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0
    }
  },
  "keywords": [
    "getheadlines",
    "rssparser",
    "webpack",
    "bootstrap"
  ],
  "dependencies": {
    "clipboard": "^1.7.1",
    "firebase": "^4.2.0",
    "firebaseui": "^2.3.0",
    "slideout": "^1.0.1"
  }
}
