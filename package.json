{
	"name": "getheadlines",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/index.js",
	"scripts": {
		"start:watch": "NODE_CONFIG_DIR=config/environments NODE_ENV=development nodemon src/index.js --exec babel-node",
		"start:dev": "NODE_CONFIG_DIR=config/environments NODE_ENV=development babel-node src/index.js",
		"build": "rm -rf ./dist/ && babel src --out-dir dist/ --copy-files",
		"prod": "NODE_CONFIG_DIR=config/environments NODE_ENV=production node dist/index.js",
		"lint": "eslint src",
		"test": "NODE_CONFIG_DIR=config/environments NODE_ENV=test mocha --require babel-core/register **/**.spec.js --recursive"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cedevita/getheadlines.git"
	},
	"keywords": [],
	"author": "Mladen Ruzicic <ruzicic@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/cedevita/getheadlines/issues"
	},
	"homepage": "https://github.com/cedevita/getheadlines#readme",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"chai": "^4.1.2",
		"eslint": "^4.18.2",
		"eslint-config-airbnb-base": "^12.1.0",
		"eslint-plugin-import": "^2.9.0",
		"mocha": "^5.0.4",
		"nodemon": "^1.17.1",
		"sequelize-cli": "^4.0.0"
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"body-parser": "^1.18.2",
		"config": "^1.30.0",
		"express": "^4.16.2",
		"pg": "^7.4.1",
		"pg-hstore": "^2.3.2",
		"sequelize": "^4.35.2",
		"winston": "^2.4.0"
	}
}
